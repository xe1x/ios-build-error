{
  "name": "ios_build_error",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "android:dev": "ENV=dev ./envscript.sh && ENVFILE=.env.dev react-native run-android",
    "android:dev:win": "SET ENV=dev ./envscript.sh && SET ENVFILE=.env.dev && react-native run-android",
    "android:prod": "ENV=prod ./envscript.sh && ENVFILE=.env.prod react-native run-android",
    "android:prod:win": "SET ENV=prod ./envscript.sh && SET ENVFILE=.env.prod && react-native run-android",
    "android:build:dev": "npm run clean:raw && npm run clean:drawable && npm run android:dev && cd android && ./gradlew clean && ENVFILE=.env.dev ./gradlew assembleRelease",
    "android:build:prod": "npm run clean:raw && npm run clean:drawable && npm run android:prod && cd android && ./gradlew clean && ENVFILE=.env.prod ./gradlew assembleRelease",
    "clean:drawable": "npm run clean:drawableHDPI && npm run clean:drawableMDPI && npm run clean:drawableXHDPI && npm run clean:drawableXXHDPI && npm run clean:drawableXXXHDPI",
    "clean:raw": "rm -rf android/app/src/main/res/raw/*",
    "clean:drawableHDPI": "rm -rf android/app/src/main/res/drawable-hdpi/*",
    "clean:drawableMDPI": "rm -rf android/app/src/main/res/drawable-mdpi/*",
    "clean:drawableXHDPI": "rm -rf android/app/src/main/res/drawable-xhdpi/*",
    "clean:drawableXXHDPI": "rm -rf android/app/src/main/res/drawable-xxhdpi/*",
    "clean:drawableXXXHDPI": "rm -rf android/app/src/main/res/drawable-xxxhdpi/*",
    "clean:ios:simulators": "xcrun simctl erase all",
    "clean:ios:CocoaPods": "cd ./ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && pod install && cd ./..",
    "ios:dev": "ENV=dev ./envscript.sh && ENVFILE=.env.dev react-native run-ios",
    "ios:prod": "ENV=prod ./envscript.sh && ENVFILE=.env.prod react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
